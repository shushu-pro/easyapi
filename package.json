{
  "name": "@shushu.pro/easyapi",
  "version": "0.1.20",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "sschen86@163.com",
  "scripts": {
    "dev": "npm run dev-client",
    "dev-client": "rollup -c ./rollup/config.dev-client.ts -w --configPlugin typescript2 --bundleConfigAsCjs",
    "build": "rimraf dist && rollup -c ./rollup/config.prod.ts --configPlugin typescript2 --bundleConfigAsCjs",
    "build-local": "rimraf dist-local && rollup -c ./rollup/config.prod.local.ts --configPlugin typescript2 --bundleConfigAsCjs",
    "types": "tsc --project ./ts/tsconfig.types.json",
    "types-local": "tsc --project ./ts/tsconfig.types.json --declarationDir ./dist-local",
    "pub": "npm run test && npm version patch && npm run build && npm run types && npm publish",
    "pub-local": "npm run test && npm run build-local && npm run types-local",
    "test": "jest test"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@shushu.pro/adapter": "^0.0.6",
    "@shushu.pro/eslint-config-base": "^0.0.3",
    "@shushu.pro/rollup-config": "^0.0.6",
    "@shushu.pro/tsconfig": "^0.0.1",
    "@types/node": "^18.11.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "ijest": "^1.0.7",
    "jest": "^29.2.1",
    "js-cookie": "^3.0.1",
    "qs": "^6.11.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.3",
    "typescript": "^4.8.4"
  },
  "files": [
    "dist/",
    "template/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "repository": "https://github.com/shushu-pro/easyapi.git",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "description": "ajax api manage",
  "keywords": [
    "shushu.pro"
  ]
}
