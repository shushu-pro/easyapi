{
  "name": "@shushu.pro/easyapi",
  "version": "0.1.19",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "sschen86@163.com",
  "scripts": {
    "dev": "npm run dev-chrome",
    "build": "rimraf dist && rollup -c ./rollup/config.prod.ts --configPlugin typescript",
    "build:local": "rimraf easyapi-local && rollup -c ./rollup/config.prod.local.ts --configPlugin typescript && tsc --project ./tsconfig/types.local.json",
    "dev-chrome": "rollup -c ./rollup/config.dev.ts -w --configPlugin typescript",
    "dev-node": "concurrently \"rollup -c ./rollup/config.dev.ts -w --configPlugin typescript\" \"nodemon --watch temp --exec node ./temp/index\"",
    "types": "tsc --project ./tsconfig/types.json",
    "deploy": "npm run test && npm version patch && npm run build && npm run types && npm publish",
    "test": "jest test"
  },
  "keywords": [
    "shushu.pro"
  ],
  "files": [
    "dist/",
    "template/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://github.com/shushu-pro/easyapi.git",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@shushu.pro/adapter": "^0.0.6",
    "@shushu.pro/eslint-config-base": "^0.0.3",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.7.23",
    "@types/qs": "^6.9.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.24.0",
    "ijest": "^1.0.7",
    "jest": "^29.0.3",
    "js-cookie": "^3.0.1",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.16",
    "qs": "^6.11.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "lodash": "^4.17.21"
  }
}
